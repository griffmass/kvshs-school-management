# Ignore generated CSS files in GitHub language stats
styles.css linguist-generated=true

# Ignore built HTML files (if in dist/ or public/)
*.html linguist-generated=true

# Ignore compiled JS files from TypeScript or build tools
dist/* linguist-generated=true
build/* linguist-generated=true
out/* linguist-generated=true
*.js linguist-generated=true

# Treat documentation files as text, not code
*.md linguist-documentation=true

# Optional: ensure consistent line endings
* text=auto
